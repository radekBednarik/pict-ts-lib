      "values": ["On", "Off"]
    }
  ],
  "constraints": [
    {
      "if": {
        "parameter": "Type",
        "condition": "=",
        "value": "FAT"
      },
      "then": {
        "parameter": "Size",
        "condition": "<=",
        "value": 4096
      }
    },
    {
      "parameter": "Size",
      "condition": "<",
      "value": 10000
    },
    {
      "if": {
        "parameter": "Cluster size",
        "condition": "IN",
        "value": [512, 1024, 2048]
      },
      "then": {
        "parameter": "Compression",
        "condition": "=",
        "value": "Off"
      }
    }
  ],
  "submodels": []
