      "values": ["Quick", "Slow"]
    },
    {
      "name": "File system",
      "values": ["FAT", "FAT32", "NTFS"]
    },
    {
      "name": "Cluster size",
      "values": [512, 1024, 2048, 4096, 8192, 16384, 32768, 65536]
    },
    {
